# Porta onde o Kestrel vai expor (opcional)
ASPNETCORE_URLS=http://localhost:5000

# MongoDB (substitua pela sua string)
MongoDb__ConnectionString=mongodb://localhost:27017
MongoDb__Database=CadastroAcoesDb

# JWT (exemplo; troque pelo segredo real)
Jwt__Key=sua-chave-de-teste-123456
Jwt__Issuer=MinhaApp
Jwt__Audience=MinhaAppClients
Jwt__ExpiresMinutes=60